{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": ["@typescript-eslint"],
  "extends": "plugin:@fui/typescript",
  "rules": {
    "quotes": ["error", "single", { "allowTemplateLiterals": true }]
  }
}
